<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/mainFragment">

    <fragment
        android:id="@+id/mainFragment"
        android:name="com.example.versatilereading.MainFragment"
        android:label="fragment_main"
        tools:layout="@layout/fragment_main" >
        <action
            android:id="@+id/action_mainFragment_to_oneTImeFragment"
            app:destination="@id/oneTImeFragment" />
        <action
            android:id="@+id/action_mainFragment_to_subscriptionFragment"
            app:destination="@id/subscriptionFragment" />
    </fragment>
    <fragment
        android:id="@+id/oneTImeFragment"
        android:name="com.example.versatilereading.OneTimeFragment"
        android:label="fragment_one_time"
        tools:layout="@layout/fragment_one_time" />
    <fragment
        android:id="@+id/subscriptionFragment"
        android:name="com.example.versatilereading.SubscriptionFragment"
        android:label="fragment_subscription"
        tools:layout="@layout/fragment_subscription" />
</navigation>